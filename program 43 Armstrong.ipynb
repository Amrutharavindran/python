   "source": [
    "#Armstrong number\n",
    "n=int(input(\"Enter a number:\"))\n",
    "sum=0\n",
    "temp=n\n",
    "while (temp>0):\n",
    "    digit=temp%10\n",
    "    sum+=digit**3\n",
    "    temp//=10\n",
    "if(n==sum):\n",
    "    print(n,\"is an armstrong number\")\n",
    "else:\n",
    "    print(n,\"is not an armstrong number\")\n",
    "    "
