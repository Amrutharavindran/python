    "#calculate digits and numbers of the string\n",
    "a=input(\"enter a string:\")\n",
    "d=0\n",
    "l=0\n",
    "for i in a:\n",
    "    if(i.isdigit()):\n",
    "        d=d+1\n",
    "    elif(i.isalpha()):\n",
    "        l=l+1\n",
    "print(\"the total letters in the string is:%d\"%l)\n",
    "print(\"the total numbers in the string is:%d\"%d)"
   ]
