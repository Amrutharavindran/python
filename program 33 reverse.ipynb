    "#reverse of a number\n",
    "n=int(input(\"enter a number:\"))\n",
    "print(\"reverse of number is:\")\n",
    "rev=0\n",
    "while(n!=0):\n",
    "    p=n%10\n",
    "    rev=rev*10+p\n",
    "    n//=10\n",
    "    print(p,end=\"\")"
