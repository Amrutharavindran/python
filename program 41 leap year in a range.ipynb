   "source": [
    "#leap year in a range\n",
    "a=int(input(\"Enter the starting year:\"))\n",
    "b=int(input(\"Enter the ending year:\"))\n",
    "print(\"leap years from\",a,\"to\",b,\"is:\")\n",
    "for i in range(a,b):\n",
    "    if(0==i%4)and(0!=i%100)or(0==i%400):\n",
    "        print(i)"
