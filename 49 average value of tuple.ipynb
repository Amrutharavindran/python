   "source": [
    "#average value of a tuple\n",
    "t=tuple(input(\"enter numbers separated by comma:\").split(\",\"))\n",
    "l=len(t)\n",
    "c=0\n",
    "for i in range(l):\n",
    "    c=int(t[i])+c\n",
    "    avg=c/l\n",
    "print(\"Average value of the numbers in the tuple:\",avg)"
   ]
