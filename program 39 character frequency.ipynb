   "source": [
    "#frequency of characters in a string\n",
    "s=input(\"Enter a string:\")\n",
    "frq={}\n",
    "for i in s:\n",
    "    if i in frq:\n",
    "        frq[i]=frq[i]+1\n",
    "        \n",
    "    else:\n",
    "        frq[i]=1\n",
    "for keys,value in frq.items():\n",
    "    print(keys,\"frequency of character\",value,'time(s)')\n",
    "    "
   ]
