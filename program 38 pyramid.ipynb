    "#pyramid\n",
    "n=int(input(\"enter a number:\"))\n",
    "print(\"pyramid:\")\n",
    "for i in range(1,n+1):\n",
    "    for j in range(1,i+1):\n",
    "        print(i*j,end=\" \")\n",
    "    print(\"\\n\")"
